package akhila.ch22;

public class emp
{
	int Id;
	String Name;
	String Dept;
	String salary;
	public emp(int id, String name, String dept, String salary) {
		super();
		Id = id;
		Name = name;
		Dept = dept;
		this.salary = salary;
	}
	@Override
	public String toString() {
		return "emp [Id=" + Id + ", Name=" + Name + ", Dept=" + Dept + ", salary=" + salary + "]";
	}
}
